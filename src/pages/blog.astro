---
const title = "Blog";
import Layout from "../layouts/Layout.astro";
const posts = Object.values(import.meta.glob("./posts/*.md", {eager: true}));

---
<Layout page_title={title}>
    Here comes my blog.
    <ul>
        {posts.map(post => (
          <li>
            <a href={post.url}>{post.frontmatter.title}</a>
          </li>
        ))}
      </ul>
</Layout>
